<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<VBox prefHeight="181.0" prefWidth="250.0" xmlns="http://javafx.com/javafx/23" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.github.devgabrielc.model.controllers.AddScreenController">
    <children>
      <AnchorPane>
         <children>
            <Button fx:id="ativoImobilizadoButton" layoutX="60.0" layoutY="50.0" mnemonicParsing="false" onAction="#handleAtivoImobilizadoScreen" prefHeight="25.0" prefWidth="130.0" text="Ativo Imobilizado">
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor></Button>
            <Button fx:id="usoConsumoButton" layoutX="60.0" layoutY="91.0" mnemonicParsing="false" onAction="#handleUsoConsumoScreen" prefHeight="25.0" prefWidth="130.0" text="Uso e Consumo">
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor></Button>
            <Button fx:id="cancelButton" layoutX="88.0" layoutY="142.0" mnemonicParsing="false" onAction="#handleCancelar" prefHeight="25.0" prefWidth="75.0" text="Cancelar">
               <cursor>
                  <Cursor fx:constant="HAND" />
               </cursor></Button>
            <Text layoutX="61.0" layoutY="28.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Tipo de Material" textAlignment="CENTER">
               <font>
                  <Font name="Calibri" size="19.0" />
               </font>
            </Text>
         </children>
      </AnchorPane>
    </children>
</VBox>
